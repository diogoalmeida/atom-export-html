{
  "name": "export-html",
  "configSchema": {
    "fontSize": {
      "type": "integer",
      "default": 12
    },
    "openBrowser": {
      "type": "boolean",
      "default": true
    },
    "style": {
      "type": "string",
      "default": "github",
      "tilte": "Stylesheet",
      "description": "Choose from [highlight.js styles.](https://github.com/isagalaev/highlight.js/tree/master/src/styles) ."
    },
    "lineNumber": {
      "type": "object",
      "properties": {
        "use": {
          "type": "boolean",
          "default": true
        },
        "styles": {
          "type": "string",
          "title": "StyleSheet",
          "default": "opacity: 0.5;"
        }
      }
    }
  },
  "main": "./lib/export-html",
  "version": "1.1.0",
  "description": "Export to HTML and open this! (You can also print)",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": "export-html:export"
  },
  "repository": "https://github.com/nobuhito/atom-export-html",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "underscore-plus": "^1.6.6",
    "highlight.js": "^8.7.0",
    "roaster": "^1.2.0"
  }
}
